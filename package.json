{
  "name": "ts-mock-proxy",
  "version": "1.0.0",
  "description": "Zero-Config mock server that generates REST API from TypeScript interfaces",
  "main": "dist/index.js",
  "bin": {
    "ts-mock-proxy": "dist/index.js"
  },
  "scripts": {
    "dev": "nodemon --exec tsx src/index.ts",
    "dev:external": "tsx src/index.ts --external-dir ../external-api-types --verbose",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist"
  },
  "keywords": [
    "mock",
    "api",
    "typescript",
    "rest",
    "faker",
    "development"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/swagger-ui-express": "^4.1.8",
    "chalk": "^4.1.2",
    "chokidar": "^3.6.0",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "intermock": "^0.2.5",
    "pluralize": "^8.0.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/pluralize": "^0.0.33",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
